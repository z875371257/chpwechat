.container{background: #FFF;}
/* 列表 */
.article_list{width: 680rpx; height:180rpx; margin:auto;border-bottom:1px solid #F7F7F7;margin-top:22rpx; }
.artleft{width:210rpx; height:158rpx;  float: left;}
.artleft image{width:100%; height:100%;  }
.artright{width:445rpx; height:138rpx;  float: left;padding: 10rpx 0 10rpx 20rpx;}
.artright-name{width:100%; height:90rpx;font-size:30rpx; overflow: hidden; color:#333; }
.article-company{width:100%; height:50rpx; margin-top:4rpx;overflow: hidden; }
.art-free{width:140rpx;height:40rpx;text-align: center;font-size:24rpx;float:left;color:#FFF;line-height: 40rpx;margin-right:20rpx}
.art-free image{width:36rpx;height:30rpx;float:left;margin-top:4rpx;}
.art-company-name{max-width:250rpx; float: left;font-size:24rpx;color:#999;overflow: hidden;height:38rpx; line-height: 38rpx;}
.company-name-more{width:30rpx;height:38rpx;float: left;}

/* 没有数据时 */
.NotPro{width:100%;text-align: center;font-size:28rpx;padding:40rpx 0;color:#999}